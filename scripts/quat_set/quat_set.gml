/**
 * Creates a quaternion
 * @param	{Real} cx		Component x
 * @param	{Real} cy		Component y
 * @param	{Real} cz		Component z
 * @param	{Real} cw		Component w
 * @returns	{Array<Real>}	(Quat)
 */
function quat_set(cx, cy, cz, cw) {

	return [cx, cy, cz, cw];

}
